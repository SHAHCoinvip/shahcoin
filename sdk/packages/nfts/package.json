{
  "name": "@shahcoin/nfts",
  "version": "1.0.0",
  "description": "SHAHCOIN NFTs SDK - SHI-30 NFT registry, metadata manager, mint/burn functions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "docs": "typedoc src/index.ts --out docs"
  },
  "dependencies": {
    "ethers": "^6.8.0",
    "ipfs-http-client": "^60.0.0",
    "multiformats": "^12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@shahcoin/core": "workspace:*",
    "@shahcoin/wallet": "workspace:*",
    "@shahcoin/types": "workspace:*"
  },
  "keywords": [
    "shahcoin",
    "nfts",
    "shi-30",
    "erc-721",
    "metadata",
    "ipfs",
    "digital-art"
  ]
}
