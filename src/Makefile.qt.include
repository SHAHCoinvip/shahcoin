#locale/foo.ts -> locale/foo.qm
QT_QM=$(QT_TS:.ts=.qm)

SECONDARY: $(QT_QM)

$(srcdir)/qt/shahcoinstrings.cpp: FORCE
	@test -n $(XGETTEXT) || echo "xgettext is required for updating translations"
	$(AM_V_GEN) cd $(srcdir); XGETTEXT=$(XGETTEXT) COPYRIGHT_HOLDERS="$(COPYRIGHT_HOLDERS)" $(PYTHON) ../share/qt/extract_strings_qt.py \
    $(libshahcoin_node_a_SOURCES) $(libshahcoin_wallet_a_SOURCES) $(libshahcoin_common_a_SOURCES) \
    $(libshahcoin_zmq_a_SOURCES) $(libshahcoin_consensus_a_SOURCES) $(libshahcoin_util_a_SOURCES) \
    $(SHAHCOIN_QT_BASE_CPP) $(SHAHCOIN_QT_WINDOWS_CPP) $(SHAHCOIN_QT_WALLET_CPP) $(SHAHCOIN_QT_H) $(SHAHCOIN_MM)

# The resulted shahcoin_en.xlf source file should follow Transifex requirements.
# See: https://docs.transifex.com/formats/xliff#how-to-distinguish-between-a-source-file-and-a-translation-file
translate: $(srcdir)/qt/shahcoinstrings.cpp $(QT_FORMS_UI) $(QT_FORMS_UI) $(SHAHCOIN_QT_BASE_CPP) $(SHAHCOIN_QT_WINDOWS_CPP) $(SHAHCOIN_QT_WALLET_CPP) $(SHAHCOIN_QT_H) $(SHAHCOIN_MM)
	@test -n $(LUPDATE) || echo "lupdate is required for updating translations"
	$(AM_V_GEN) QT_SELECT=$(QT_SELECT) $(LUPDATE) -no-obsolete -I $(srcdir) -locations relative $^ -ts $(srcdir)/qt/locale/shahcoin_en.ts
	@test -n $(LCONVERT) || echo "lconvert is required for updating translations"
	$(AM_V_GEN) QT_SELECT=$(QT_SELECT) $(LCONVERT) -drop-translations -o $(srcdir)/qt/locale/shahcoin_en.xlf -i $(srcdir)/qt/locale/shahcoin_en.ts
	@$(SED) -i.old -e 's|source-language="en" target-language="en"|source-language="en"|' -e '/<target xml:space="preserve"><\/target>/d' $(srcdir)/qt/locale/shahcoin_en.xlf
	@rm -f $(srcdir)/qt/locale/shahcoin_en.xlf.old

$(QT_QRC_LOCALE_CPP): $(QT_QRC_LOCALE) $(QT_QM)
	@test -f $(RCC) || (echo "rcc $(RCC) not found, but is required for generating qrc cpp files"; exit 1)
	@cp -f $< $(@D)/temp_$(<F)
	$(AM_V_GEN) QT_SELECT=$(QT_SELECT) $(RCC) -name shahcoin_locale --format-version 1 $(@D)/temp_$(<F) > $@
	@rm $(@D)/temp_$(<F)

$(QT_QRC_CPP): $(QT_QRC) $(QT_FORMS_H) $(QT_RES_FONTS) $(QT_RES_ICONS) $(QT_RES_ANIMATION)
	@test -f $(RCC) || (echo "rcc $(RCC) not found, but is required for generating qrc cpp files"; exit 1)
	$(AM_V_GEN) QT_SELECT=$(QT_SELECT) $(RCC) -name shahcoin --format-version 1 $< > $@

CLEAN_QT = $(nodist_qt_libshahcoinqt_a_SOURCES) $(QT_QM) $(QT_FORMS_H) qt/*.gcda qt/*.gcno qt/temp_shahcoin_locale.qrc

CLEANFILES += $(CLEAN_QT)

shahcoin_qt_clean: FORCE
	rm -f $(CLEAN_QT) $(qt_libshahcoinqt_a_OBJECTS) $(qt_shahcoin_qt_OBJECTS) qt/shahcoin-qt$(EXEEXT) $(LIBSHAHCOINQT)

shahcoin_qt : qt/shahcoin-qt$(EXEEXT)

